  ],
  "contributes": {
    "commands": [
      {
        "command": "flutter-snippets.showCatalog",
        "title": "ðŸ¤– Show Flutter Snippets Catalog",
        "category": "Flutter Snippets"
      }
    ],
    "configuration": {
      "title": "Flutter Snippets Pro",
      "properties": {
        "flutterSnippets.enableCopilotEnhancement": {
          "type": "boolean",
          "default": true,
          "description": "Enable GitHub Copilot enhancement features"
        },
        "flutterSnippets.autoTriggerSuggestions": {
          "type": "boolean", 
          "default": true,
          "description": "Automatically trigger snippet suggestions based on context"
        },
        "flutterSnippets.showIntelligentHovers": {
          "type": "boolean",
          "default": true,
          "description": "Show intelligent hover information for Flutter patterns"
        }
      }
    },
    "grammars": [
      {
        "language": "dart",
        "scopeName": "source.dart.flutter.enhanced",
        "path": "./syntaxes/flutter-enhanced.json",
        "injectTo": ["source.dart"]
